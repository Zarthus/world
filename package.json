{
  "dependencies": {
    "bootstrap-icons": "^1.8.1",
    "bulma": "^0.9.3",
    "sass": "^1.51.0"
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": false,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "script"
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "sort-imports": "error",
      "func-names": "off",
      "no-console": "off",
      "eqeqeq": "warn",
      "no-else-return": "warn",
      "yoda": "warn",
      "max-len": [
        "error",
        {
          "code": 160
        }
      ]
    }
  }
}
