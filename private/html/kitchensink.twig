{% block content %}
<div class="container">
    <div class="box">
        <div class="content">
            <h3>Control Panel</h3>
            <div class="columns is-multiline">
            {% for val in [
                "is-primary", "is-link", "is-info", "is-success", "is-warning", "is-danger",
                "has-text-primary", "has-text-link", "has-text-info", "has-text-success", "has-text-warning", "has-text-danger",
                "has-background-primary", "has-background-link", "has-background-info", "has-background-success", "has-background-warning", "has-background-danger",
                "has-background-primary", "has-background-link", "has-background-info", "has-background-success", "has-background-warning", "has-background-danger",
                "is-small", "is-medium", "is-large",
                "is-outlined", "is-loading"]
            %}

                <div class="column">
                    <button class="button {{ val }}" value="{{ val }}" onclick="setModifier('{{ val }}')">{{ val }}</button>
                    <button class="button {{ val }}-dark" value="{{ val }}-dark" onclick="setModifier('{{ val }}-dark')">{{ val }}-dark</button>
                    <button class="button {{ val }}-light" value="{{ val }}-light" onclick="setModifier('{{ val }}-light')">{{ val }}-light</button>
                    <br>
                </div>
            {% endfor %}

                <div class="column">
                    <button class="button" value="is-danger" onclick="setModifier('none-set')">Unset Modifier</button>
                    <button class="button" onclick="toggleCollapse('basic-html')">Toggle Collapse Basic HTML</button>
                    <button class="button" onclick="toggleCollapse('bulma-elements')">Toggle Collapse Bulma Elements</button>
                    <button class="button" onclick="toggleCollapse('bulma-components')">Toggle Collapse Bulma Components</button>
                    <button class="button" onclick="toggleCollapse('bulma-forms')">Toggle Collapse Bulma Forms</button>
                </div>
            </div>
        </div>
    </div>
    <script>
        let activeModifier = null;
        function setModifier(mod) {
            const target = document.getElementById('js-apply');

            for (const divs of target.children) {
                for (const elem of divs.children) {
                    if (null !== activeModifier) {
                        elem.classList.remove(activeModifier);
                    }
                    elem.classList.add(mod);
                }
            }
            activeModifier = mod;
            console.log('Set active modifier to ' + mod);
        }

        function toggleCollapse(mod) {
            const cl = document.getElementById(mod).classList;

            if (cl.contains('is-hidden')) {
                cl.remove('is-hidden');
            } else {
                cl.add('is-hidden');
            }
        }
    </script>
</div>
